{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security.Bearer": {
    "Authority": "https://localhost:{sts_port}",
    "Audience": "api"
  },
  "MassTransit": {
    "RetryInterval": {
      "RetryCount": 10,
      "Interval": "00:00:05"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=BRUNOAPIDb;Integrated Security=True;TrustServerCertificate=True;MultipleActiveResultSets=True"
  }
}